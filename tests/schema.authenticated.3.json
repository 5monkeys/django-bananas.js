    "components": {
        "schemas": {
            "Form": {
                "required": [
                    "boolean",
                    "choices",
                    "date",
                    "datetime",
                    "integer",
                    "multiple_choices",
                    "text"
                ],
                "type": "object",
                "properties": {
                    "boolean": {
                        "title": "Boolean",
                        "type": "boolean"
                    },
                    "choices": {
                        "title": "Choices",
                        "type": "string",
                        "enum": [
                            "se",
                            "no",
                            "fi",
                            "dk"
                        ]
                    },
                    "date": {
                        "title": "Date",
                        "type": "string",
                        "format": "date"
                    },
                    "datetime": {
                        "title": "Datetime",
                        "type": "string",
                        "format": "date-time"
                    },
                    "integer": {
                        "title": "Integer",
                        "type": "integer"
                    },
                    "multiple_choices": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "se",
                                "no",
                                "fi",
                                "dk"
                            ]
                        }
                    },
                    "text": {
                        "title": "Text",
                        "minLength": 1,
                        "type": "string"
                    }
                }
            },
            "PasswordChange": {
                "required": [
                    "new_password1",
                    "new_password2",
                    "old_password"
                ],
                "type": "object",
                "properties": {
                    "new_password1": {
                        "title": "Nytt lösenord",
                        "minLength": 1,
                        "type": "string",
                        "description": "['Ditt lösenord kan inte vara alltför lik din personliga information.', 'Ditt lösenord måste innehålla minst 8 tecken.', 'Ditt lösenord kan inte vara ett allmänt använt lösenord.', 'Ditt lösenord kan inte bara vara numeriskt.']"
                    },
                    "new_password2": {
                        "title": "Bekräfta nytt lösenord",
                        "minLength": 1,
                        "type": "string"
                    },
                    "old_password": {
                        "title": "Gammalt lösenord",
                        "minLength": 1,
                        "type": "string"
                    }
                }
            },
            "User": {
                "type": "object",
                "properties": {
                    "email": {
                        "title": "Email",
                        "minLength": 1,
                        "type": "string",
                        "readOnly": true
                    },
                    "full_name": {
                        "title": "Full name",
                        "minLength": 1,
                        "type": "string",
                        "description": "Falls back to username, if not implemented or empty",
                        "readOnly": true
                    },
                    "groups": {
                        "type": "array",
                        "readOnly": true,
                        "items": {
                            "type": "string"
                        }
                    },
                    "id": {
                        "title": "ID",
                        "type": "integer",
                        "readOnly": true
                    },
                    "is_superuser": {
                        "title": "Is superuser",
                        "type": "boolean",
                        "readOnly": true
                    },
                    "permissions": {
                        "type": "array",
                        "description": "Permissions that the user has, both through group and user permissions.",
                        "readOnly": true,
                        "items": {
                            "type": "string"
                        }
                    },
                    "username": {
                        "title": "Username",
                        "minLength": 1,
                        "type": "string",
                        "readOnly": true
                    }
                }
            },
            "UserDetails": {
                "required": [
                    "username"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "title": "E-postadress",
                        "maxLength": 254,
                        "type": "string",
                        "format": "email"
                    },
                    "full_name": {
                        "title": "Full name",
                        "minLength": 1,
                        "type": "string",
                        "readOnly": true
                    },
                    "id": {
                        "title": "ID",
                        "type": "integer",
                        "readOnly": true
                    },
                    "is_staff": {
                        "title": "Personalstatus",
                        "type": "boolean",
                        "description": "Avgör om användaren kan logga in på denna adminsida."
                    },
                    "url": {
                        "title": "Url",
                        "type": "string",
                        "format": "uri",
                        "readOnly": true
                    },
                    "username": {
                        "title": "Användarnamn",
                        "maxLength": 150,
                        "minLength": 1,
                        "pattern": "^[\\w.@+-]+$",
                        "type": "string",
                        "description": "Obligatoriskt. 150 tecken eller färre. Endast bokstäver, siffror och @/./+/-/_."
                    }
                }
            }
        },
