{
  "name": "django-bananas",
  "version": "3.4.2",
  "license": "MIT",
  "author": "Jonas Lundberg",
  "repository": "5monkeys/django-bananas.js",
  "description": "React admin frontend for Django.",
  "main": "index.js",
  "private": true,
  "keywords": [
    "admin",
    "bananas",
    "django",
    "django-admin",
    "react"
  ],
  "scripts": {
    "start": "webpack serve --config app/webpack.config.js",
    "watch": "jest --watchAll --verbose",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "jest": "jest --verbose",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "buildwatch": "babel src --watch --out-dir dist",
    "build": "babel --delete-dir-on-start src --out-dir dist && cp LICENSE README.md dist/ && sed \"/\\\"private\\\":\\s*true/d\" package.json > dist/package.json",
    "test": "npm run eslint && npm run coverage && npm run build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.8",
    "@material-ui/pickers": "^3.1.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-beta.2",
    "deprecated-prop-type": "^1.0.0",
    "history": "^4.9.0",
    "js-logger": "^1.6.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.5.7",
    "swagger-client": "3.8.22"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "final-form": "^4.16.1",
    "final-form-arrays": "^1.1.2",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-arrow-functions": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@date-io/core": "2.10.7",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "11.2.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "coveralls": "3.1.0",
    "eslint": "7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fetch-mock": "7.3.3",
    "file-loader": "6.2.0",
    "final-form": "4.16.1",
    "final-form-arrays": "1.1.2",
    "gh-pages": "2.0.1",
    "gravatar": "1.8.0",
    "html-webpack-plugin": "5.0.0",
    "jest": "26.6.3",
    "prettier": "1.18.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.2",
