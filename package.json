{
  "name": "django-bananas",
  "version": "1.3.1",
  "license": "MIT",
  "author": "Jonas Lundberg",
  "repository": "5monkeys/django-bananas.js",
  "description": "React admin frontend for Django.",
  "main": "index.js",
  "private": true,
  "keywords": [
    "admin",
    "bananas",
    "django",
    "django-admin",
    "react"
  ],
  "scripts": {
    "watch": "jest --watchAll --verbose",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --write \"*.md\"",
    "doctoc": "doctoc --maxlevel 3 README.md",
    "jest": "jest --verbose",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "buildwatch": "babel src --watch --out-dir dist",
    "build": "babel --delete-dir-on-start src --out-dir dist && cp LICENSE README.md dist/ && sed \"/\\\"private\\\":\\s*true/d\" package.json > dist/package.json",
    "test": "npm run eslint && npm run coverage && npm run build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.1.0",
    "date-fns": "^2.0.0-alpha.27",
    "final-form": ">=4.12.0",
    "final-form-arrays": ">=1.1.2",
    "history": "^4.7.2",
    "js-logger": "^1.6.0",
    "prop-types": "^15.5.7",
    "react-final-form": ">=4.1.0",
    "react-final-form-arrays": "2.0.1",
    "swagger-client": "3.8.22"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "@material-ui/core": "4.1.3",
    "@material-ui/icons": "4.2.1",
    "@material-ui/pickers": "^3.1.2",
    "react": ">=16.8.6",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-jsx": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "classnames": "^2.2.6",
    "@date-io/core": "^1.3.8",
    "@material-ui/core": "4.1.3",
    "@material-ui/icons": "4.2.1",
    "@material-ui/pickers": "^3.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "coveralls": "3.0.3",
    "doctoc": "1.4.0",
    "eslint": "5.10.0",
    "eslint-config-lydell": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-simple-import-sort": "2.0.0",
    "fetch-mock": "7.3.1",
    "gh-pages": "^2.0.1",
    "jest": "24.1.0",
    "jest-dom": "^3.1.3",
    "prettier": "1.15.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jss": "10.0.0-alpha.21",
    "react-test-renderer": "16.6.3",
    "react-testing-library": "6.0.0"
  }
}
